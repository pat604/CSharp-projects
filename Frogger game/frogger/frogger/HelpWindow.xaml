<Window x:Class="frogger.HelpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="HelpWindow" Height="400" Width="400" Loaded="Window_Loaded_1" 
        WindowStyle="None" ResizeMode="NoResize">
    <Grid Background="Black">
        <TextBlock x:Name="textblock" Margin="30,105,30,79" TextWrapping="Wrap" Foreground="#FF529B60" FontFamily="Kristen ITC" TextAlignment="Center" />
        <Canvas x:Name="canvasfrogger" HorizontalAlignment="Left" Height="65" Margin="30,24,0,0" VerticalAlignment="Top" Width="239" />
        <Canvas x:Name="canvashelp" HorizontalAlignment="Left" Height="44" Margin="262,45,0,0" VerticalAlignment="Top" Width="108"/>
        <Button Content="OK" Margin="100,0,100,28" FontSize="15" Click="Button_Click_1" MaxHeight="40" MaxWidth="200" FontFamily="Kristen ITC" FontWeight="ExtraBlack" IsManipulationEnabled="True" Height="35" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
